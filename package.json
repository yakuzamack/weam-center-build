{
  "name": "weam-center",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run postbuild",
    "postbuild": "cp out/en.html out/en/index.html && cp out/ar.html out/ar/index.html",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0 .",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "14.2.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "clsx": "2.1.1"
  },
  "devDependencies": {
    "typescript": "5.6.2",
    "@types/react": "18.3.7",
    "@types/node": "20.12.7",
    "tailwindcss": "3.4.10",
    "postcss": "8.4.47",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.13",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "prettier": "3.3.3",
    "lint-staged": "15.2.9",
    "husky": "9.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,css,md,json}": [
      "prettier --write"
    ]
  }
}
